{
   "manifest_version": 3,
   "name": "The Monitoring Shop's AppDynamics RUM and Analytics Injector",
   "version": "0.1.0",
   "permissions": [
      "activeTab",
      "tabs",
      "scripting"
   ],
   "icons": {
      "128": "icon.png"
   },
   "host_permissions": [
      "*://*/*",
      "<all_urls>"
   ],
   // "background": {
   //    "service_worker": "tms_background.js",
   //    "type": "module"
   // },
   "content_scripts": [
      {
         "matches": [
            "<all_urls>"
         ],
         "js": [
            "tms_adrum_content.js"
         ],
         "run_at": "document_start",
         "all_frames": true
      }
   ],
   "web_accessible_resources": [
      {
         "resources": [
            "tms_adrum_content.js",
            "/adrum_configs/*.js",
            "adrum.js"
         ],
         "matches": [
            "<all_urls>"
         ]
      }
   ]
}