{
"update_url": "https://clients2.google.com/service/update2/crx",

   "background": {
      "scripts": [ "chance.min.js", "background.js" ]
   },
   "browser_action": {
      "default_icon": "icon.png",
      "default_popup": "popup.html"
   },
   "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
   "description": "",
   "icons": {
      "128": "icon.png"
   },
   "manifest_version": 2,
   "name": "The Monitoring Shop's AppDynamics RUM and Analytics Injector",
   "options_page": "options.html",
   "permissions": [
      "activeTab",
      "storage"
   ],
   "optional_permissions": [
      "<all_urls>",
      "webRequest",
      "webRequestBlocking"
   ],
   "version": "0.1.0",
   "web_accessible_resources": [ "chance.min.js" ]
}
